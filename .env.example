# ======================
# Imagen Apex Configuration
# ======================
# Copy this file to .env and fill in your credentials

# Google Cloud Configuration
GCP_PROJECT_ID=your-gcp-project-id
GCP_REGION=us-central1

# SAM 3D Endpoint
# After deployment, set this to your Cloud Run or Vertex AI endpoint URL
SAM3D_ENDPOINT=http://localhost:8080/predict
SAM3D_API_KEY=sam3d-demo-key-2024

# HuggingFace Token
# Required for downloading SAM 3D model weights
# Get your token at: https://huggingface.co/settings/tokens
HF_TOKEN=your-huggingface-token

# Optional: Image Generation Settings
IMAGE_ASPECT_RATIO=1:1
PREFER_NANO_BANANA=true
